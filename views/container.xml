<?xml version="1.0"?>
<openerp>
    <data>





<!-- FORM --> 
		<record model="ir.ui.view" id="container_form_view">

			<field name="name">container.form</field>

			<field name="model">clean.container</field>

			<field name="arch" type="xml">

				<form string="container Form">

					<!-- Header -->
					<header>

<!-- Left -->
                        <!-- Encode Error -->
                        <button 
                                type="object"
                                class="oe_highlight"

                                name="reproduce_encode_error"

                                string="REPRODUCE Encode Error"
                        />

                        <!-- Fix Procurement Orders -->
                        <button 
                                type="object"
                                class="oe_highlight"

                                name="fix_procurements"

                                string="FIX Procurement Orders"
                        />


                        <!-- Fix Stock Moves -->
                        <button 
                                type="object"
                                class="oe_highlight"

                                name="fix_stock"

                                string="FIX Stock Moves"
                        />

                        <!-- Validate Product Templates -->
                        <button 
                                type="object"
                                class="oe_highlight"

                                name="validate_product_templates"

                                string="VALIDATE Product Templates"
                        />

					</header>



					<!-- Sheet -->  
					<sheet>

                        <!-- Padding -->
                        <group>
                            <group>
                                <field name="name"/>
                            </group>
                            <group>
                            </group>
                        </group>


                        <!-- Notebook --> 
<!--
                        <notebook>  
                            <page string="Products">
                            </page>
                        </notebook>
-->
                    </sheet>
                </form>
            </field>
        </record>



    </data>
</openerp>
